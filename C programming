#include <stdio.h>
int main()
{
int a[10]={1,2,2,3,4,5,6,7,8,9},size=10;
printf ("Enter the position :");
int pos;
scanf("%d",&pos);
int i,count=0;
printf ("\nThe number to be Shifted : ");
for (i=pos-1;i<size-1 ;i++ ){
        printf (" %d",a[i]);
        count ++;
  a[i]=a[i+1];
}
a[size-1]=0;
printf ("\nNumber of Shifting : %d",count);
printf ("\nAfter Deletion array will be : ");
for (i=0;i<size-1 ; i++ )
printf ("%d ",a[i]);

return 0;
}

